def tensor2array(x,outpath,OutPutName):
    x = np.array(x[0,0,:,:])
    
    # Write to TIFF
    kwargs = raster_Lhasa_MNDWI.meta
    kwargs.update(dtype=rasterio.float32,count=1,compress='lzw')
    
    with rasterio.open(os.path.join(outpath, str(OutPutName)+'.tif'), 'w', **kwargs) as dst:
        dst.write_band(1, x.astype(rasterio.float32))
    
    return()
